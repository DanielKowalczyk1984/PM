{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "PowerShell Launch Script",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/scripts/run_experiments.ps1",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "(Windows) Launch",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/bin/PM.exe",
			"args": [
				"--version"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"console": "integratedTerminal"
		},
		{
			"name": "Launch",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "PM.exe",
			"args": [
				"--json","${workspaceFolder}/settings/default_settings.json", "${workspaceFolder}/instances/wt040/wt040_001.dat", "4"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}/build/Debug",
			"environment": [],
			"console": "integratedTerminal"
		}
	]
}